{
  "name": "@botfi/env",
  "description": "T3 Env",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test:unit": "echo 'Unit tests are not implemented yet'",
    "test:it": "echo 'Integration tests are not implemented yet'"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ]
  },
  "files": [
    "src"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "peerDependencies": {
    "zod": "^4.0.5"
  },
  "dependencies": {
    "@t3-oss/env-core": "0.13.8",
    "@t3-oss/env-nextjs": "0.13.8",
    "adler-32": "1.3.1"
  },
  "devDependencies": {
    "@botfi/tsconfig": "workspace:*",
    "@types/node": "^22",
    "typescript": "5.8.3",
    "zod": "4.0.5"
  }
}
